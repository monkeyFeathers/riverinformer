    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "babelfy": "^1.1.0",
    "babelify": "^6.3.0",
    "del": "^1.2.0",
    "envify": "^3.4.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.1.0",
    "gulp-flatten": "^0.1.1",
    "gulp-util": "^3.0.6",
    "jest-cli": "^0.4.18",
    "object-assign": "^3.0.0",
    "reactify": "^1.1.1",
    "run-sequence": "^1.1.0",
    "vinyl-source-stream": "^1.0.0",
